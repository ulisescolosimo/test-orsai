{
    "version": 2,
    "builds": [
        {
            "src": "config/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "config/server.js"
        }
    ],
    "env": {
        "NODE_ENV": "development"
    }
}